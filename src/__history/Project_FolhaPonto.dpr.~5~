program Project_FolhaPonto;

uses
  Vcl.Forms,
  FormTelaLogin in 'FormTelaLogin.pas' {TelaLogin},
  Vcl.Themes,
  Vcl.Styles,
  DataConexao in 'DataConexao.pas' {DataModule1: TDataModule},
  DataTelaLogin in 'DataTelaLogin.pas' {DataModule2: TDataModule};

{$R *.res}

begin
  Application.Initialize;
  Application.MainFormOnTaskbar := True;
  Application.CreateForm(TTelaLogin, TelaLogin);
  Application.CreateForm(TDataModule1, DataModule1);
  Application.CreateForm(TDataModule2, DataModule2);
  Application.Run;
end.
